PORT=3001
MONGODB_URI=mongodb://localhost:27017/sage_ai

# Google Calendar API Configuration (Service Account)
# Path to your service account JSON key file
GOOGLE_SERVICE_ACCOUNT_PATH=./service-account-key.json

# The Google Calendar ID to sync with (use the calendar email address)
# This should be the calendar ID from your Google account that you want to access
# Example: your-calendar-id@group.calendar.google.com
# Or use 'primary' for the main calendar of the service account
GOOGLE_CALENDAR_ID=primary